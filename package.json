{
  "name": "@nearmap/olr",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": ">=9.0.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:nearmap/olr.git"
  },
  "bugs": "https://github.com/nearmap/olr/issues",
  "homepage": "https://github.com/nearmap/olr",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "echo TODO",
    "build": "run-s build:*",
    "build:pkg": "babel src --out-dir build/pkg --ignore '**/*.test.js'",
    "build:files": "cp ./package.json build/pkg/ && cp ./README.md build/pkg/",
    "test": "run-s -s lint jest",
    "lint": "run-s -s lint:*",
    "lint:js": "eslint --report-unused-disable-directives --ignore-path .gitignore .",
    "lint:md": "remark --no-stdout --use remark-lint *.md",
    "jest": "jest --config ./jest.config.json --verbose --runInBand --no-cache ",
    "preci": "npm --loglevel=warn install",
    "ci": "run-s -s clean test build",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.1",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.1",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "@babel/runtime": "^7.0.0-rc.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^23.4.2",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.2.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.5.1",
    "jest-cli": "^23.4.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.1",
    "remark-preset-lint-recommended": "3.0.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.8"
  },
  "dependencies": {
    "ol": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
